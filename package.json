{
  "name": "@mentormate/node-cli",
  "version": "1.0.2",
  "description": "Node CLI",
  "bin": {
    "node-cli": "bin/node-cli"
  },
  "repository": {
    "url": "https://github.com/MentorMate/node-project-cli"
  },
  "scripts": {
    "format": "eslint \"**/*.{js,jsx}\" --fix --cache && prettier \"**/*.{js,jsx,json}\" --write --cache --cache-strategy metadata --cache-location .prettiercache",
    "test": "jest",
    "watch": "jest --watch",
    "snapupdate": "jest --updateSnapshot",
    "coverage": "jest --coverage"
  },
  "files": [
    "assets",
    "src",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "gluegun": "^5.1.2"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.8",
    "pretty-quick": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
